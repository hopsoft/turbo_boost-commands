  <footer>
    <small>
      Message →
      <% if @message %>
        <%= tag.ins @message, data: data %>
      <% else %>
        <%= tag.del "...", data: data %>
      <% end %>
      <br>
      Request Format → <code><%= request.format %></code>
      <br>
      View Template → <code><%= Current.template %></code>
    </small>
  </footer>
